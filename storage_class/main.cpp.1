#include <iostream>
using namespace std;

// function declaration
void func(void);

static int count = 10; // global variable

int main() {
  while (count--) {
    func();
  }
}

void func(void) {
  static int i = 5; // local static variable
  i++;
  cout << "i is " << i;
  cout << "and count is " << count << endl;
}
