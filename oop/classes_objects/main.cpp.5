#include <iostream>
using namespace std;

class Box {
protected:
  double width;
};

class SmallBox : Box {
public:
  void setSmallWidth(double wid);
  double getSmallWidth();
};

double SmallBox::getSmallWidth() { return width; }

void SmallBox::setSmallWidth(double wid) { width = wid; }

int main() {
  SmallBox sbox;

  sbox.setSmallWidth(5.0);
  cout << "Width of box : " << sbox.getSmallWidth() << endl;
}